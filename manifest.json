{
  "manifest_version": 2,

  "name": "I'm Feeling Lucky",
  "description": "Use I'm feeling lucky right from the Omnibox or from highlighted text",
  "version": "0.0.1",
  "author": "john.n.sylvain@gmail.com",

  "omnibox": {
    "keyword": "\\"
  },

  "icons": {
    "48": "icons/icon48.png",
    "98": "icons/icon98.png",
    "172": "icons/icon172.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus"
  ]
}
